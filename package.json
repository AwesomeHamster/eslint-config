{
  "name": "@hamster-bot/eslint-config",
  "version": "1.0.5",
  "description": "Common ESLint configuration used in Hamster",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaikoTan/eslint-config.git"
  },
  "author": {
    "name": "Maiko Tan",
    "url": "https://github.com/MaikoTan",
    "email": "maiko.tan.coding@gmail.com"
  },
  "license": "CC-BY-NC-4.0",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js"
    },
    "./node": {
      "require": "./dist/node.js"
    },
    "./typescript": {
      "require": "./dist/typescript.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc"
  },
  "prettier": "@hamster-bot/prettier-config",
  "eslintConfig": {
    "extends": [
      "./dist/index.js"
    ]
  },
  "dependencies": {
    "eslint-config-standard": "^17.1.0"
  },
  "devDependencies": {
    "@hamster-bot/prettier-config": "*",
    "@types/eslint": "^8.56.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.3.0",
    "eslint-plugin-n": "^17.3.1",
    "eslint-plugin-promise": "^6.1.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.3.0",
    "eslint-plugin-n": "^17.3.1",
    "eslint-plugin-promise": "^6.1.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-mocha": {
      "optional": true
    },
    "eslint-plugin-n": {
      "optional": true
    }
  }
}
